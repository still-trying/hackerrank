#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    
    
     int t;
    scanf("%d", &t);
    while (t--) {
        int n, k;
        scanf("%d %d", &n, &k);
        int cnt[105] = {0};
        cnt[0] = 1;
        int sum = 0;
        long res = 0;
        for (int i = 0; i < n; i++) {
            int num;
            scanf("%d", &num);
            sum = (sum + num) % k;
            if (sum < 0) sum += k; // Handle negative mod
            res += cnt[sum];
            cnt[sum]++;
        }
        printf("%ld\n", res);
    }
    return 0;
}
